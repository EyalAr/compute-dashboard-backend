    "aws-sdk-mock": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-sorted": "^0.2.0",
    "eslint": "^5.16.0",
    "json-schema-faker": "^0.5.0-rc16",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10"
